AA,OADA;AAEA,gBAAA,IAAA,EAAA,iBAFA;AAGA,gBAAA,OAAA,EAAA,QAHA;AAIA,gBAAA,IAAA,EAAA;AAJA;AAHA,aAAA;AAUA;AACA,SA5CA,EA6CA,KA7CA,CA6CA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,SA/CA;AAgDA,OApDA,MAoDA;AACA,aAAA,MAAA;AACA;AACA,KA9HA;AA+HA,IAAA,YA/HA,0BA+HA;AACA,WAAA,WAAA,CAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA,CACA,CADA;AAEA;AAnIA;AA3DA,CAAA","sourcesContent":["<template>\n  <b-form @submit.prevent>\n    <b-row>\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Title\"\n          label-for=\"v-title\"\n        >\n          <b-form-input\n            id=\"v-title'\"\n            v-model=\"formValues.title\"\n          />\n        </b-form-group>\n      </b-col>\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Attachment (*).\"\n          label-for=\"attachment\"\n        >\n          <b-form-file\n            id=\"attachment\"\n            v-model=\"formValues.image\"\n            accept=\"image/* , .pdf\"\n            placeholder=\"Upload attachment...\"\n            drop-placeholder=\"Drop file here...\"\n            @change=\"imageChange($event)\"\n          />\n        </b-form-group>\n      </b-col>\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Description\"\n          label-for=\"v-description\"\n        >\n          <b-form-textarea\n            id=\"v-description'\"\n            v-model=\"formValues.description\"\n          />\n        </b-form-group>\n      </b-col>\n      <!-- submit and reset -->\n      <b-col md=\"12\">\n        <b-button\n          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n          type=\"submit\"\n          variant=\"primary\"\n          class=\"mr-1\"\n          :disabled=\"isSubmitting\"\n          @click=\"imageUpload\"\n        >\n          <b-spinner\n            v-if=\"isSubmitting\"\n            small\n          />\n          <span v-if=\"isSubmitting\">Please Wait</span>\n\n          <span v-if=\"!isSubmitting\">Update</span>\n        </b-button>\n      </b-col>\n    </b-row>\n  </b-form>\n</template>\n\n<script>\nimport {\n  BRow,\n  BCol,\n  BFormGroup,\n  BFormInput,\n  BFormCheckbox,\n  BForm,\n  BButton,\n  BFormDatepicker,\n  BFormTimepicker,\n  BCard,\n  BSpinner,\n  BFormFile,\n  BFormTextarea,\n  BInputGroupAppend,\n  BInputGroup,\n} from 'bootstrap-vue'\nimport Ripple from 'vue-ripple-directive'\nimport vSelect from 'vue-select'\nimport axios from '@axios'\nimport { heightTransition } from '@core/mixins/ui/transition'\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\nimport { VueAutosuggest } from 'vue-autosuggest'\n\nimport Cleave from 'vue-cleave-component'\nimport 'cleave.js/dist/addons/cleave-phone.us'\nimport axiosIns from '@/libs/axios'\n\nexport default {\n  components: {\n    BRow,\n    BCol,\n    BFormGroup,\n    BFormInput,\n    BFormCheckbox,\n    BForm,\n    BButton,\n    BFormDatepicker,\n    BCard,\n    BSpinner,\n    BFormFile,\n    BFormTextarea,\n    BInputGroupAppend,\n    Cleave,\n    BInputGroup,\n    BFormTimepicker,\n\n  },\n  directives: {\n    Ripple,\n  },\n\n  mixins: [heightTransition],\n\n  data() {\n    return {\n      options: {\n        date: {\n          date: true,\n          delimiter: '-',\n          datePattern: ['Y', 'm', 'd'],\n        },\n      },\n\n      isSubmitting: false,\n\n      formValues: {\n        id: '',\n        title: '',\n        image: '',\n        description: '',\n        isNewFile: false,\n      },\n      file: '',\n    }\n  },\n  mounted() {\n    this.initTrHeight()\n    this.formValues.id = this.$route.params.id\n    this.getOnBoardig()\n  },\n  created() {\n    window.addEventListener('resize', this.initTrHeight)\n  },\n  destroyed() {\n    window.removeEventListener('resize', this.initTrHeight)\n  },\n  methods: {\n    Submit() {\n      this.isSubmitting = true\n      console.log('submit', this.formValues)\n      axios\n        .post('/onboarding/update_on_boarding', this.formValues)\n        .then(response => {\n          if (response.data.hasOwnProperty('success')) {\n            if (response.data.success === true) {\n              console.log(response.data.data)\n\n              this.$router.replace('/onboardings').then(() => {\n                this.$toast({\n                  component: ToastificationContent,\n                  position: 'top-right',\n                  props: {\n                    title: response.data.message,\n                    icon: 'EditIcon',\n                    variant: 'success',\n                  },\n                })\n              })\n            } else {\n              this.isSubmitting = false\n\n              this.$toast({\n                component: ToastificationContent,\n                position: 'top-right',\n                props: {\n                  title: response.data.message,\n                  icon: 'AlertCircleIcon',\n                  variant: 'danger',\n                },\n              })\n            }\n          } else {\n            this.isSubmitting = false\n\n            this.$toast({\n              component: ToastificationContent,\n              position: 'top-right',\n              props: {\n                title: 'Error',\n                icon: 'AlertCircleIcon',\n                variant: 'danger',\n                text: 'Something went wrong, try again later',\n              },\n            })\n          }\n        })\n        .catch(error => {\n          console.error(error)\n        })\n    },\n    getOnBoardig() {\n      console.log(this.$route.params.id)\n      const appendWithurl = `onboarding/get_on_boarding_by_id/${this.$route.params.id}`\n\n      axios.get(appendWithurl).then(response => {\n        console.log(response.data)\n        this.formValues = response.data.data\n        this.initTrHeight()\n        console.log('Category Fetched')\n      }).catch(error => {\n        console.error(error)\n      })\n    },\n    imageChange(e) {\n      this.file = e.target.files[0]\n    },\n    imageUpload() {\n      if (this.file !== '') {\n        const formData = new FormData()\n        formData.append('image', this.file)\n        console.log(formData)\n        axios\n          .post('/admin/uploadFile', formData)\n          .then(response => {\n            if (response.data.hasOwnProperty('success')) {\n              if (response.data.success === true) {\n                this.formValues.image = response.data.filePATH\n                this.Submit()\n                console.log('image Url', this.formValues.image)\n                this.$toast({\n                  component: ToastificationContent,\n                  position: 'top-right',\n                  props: {\n                    title: response.data.message,\n                    icon: 'EditIcon',\n                    variant: 'success',\n                  },\n                })\n              } else {\n                this.isSubmitting = false\n\n                this.$toast({\n                  component: ToastificationContent,\n                  position: 'top-right',\n                  props: {\n                    title: response.data.message,\n                    icon: 'AlertCircleIcon',\n                    variant: 'danger',\n                  },\n                })\n              }\n            } else {\n              this.isSubmitting = false\n\n              this.$toast({\n                component: ToastificationContent,\n                position: 'top-right',\n                props: {\n                  title: 'Error',\n                  icon: 'AlertCircleIcon',\n                  variant: 'danger',\n                  text: 'Something went wrong, try again later',\n                },\n              })\n            }\n          })\n          .catch(error => {\n            console.error(error)\n          })\n      } else {\n        this.Submit()\n      }\n    },\n    initTrHeight() {\n      this.trSetHeight(null)\n      this.$nextTick(() => {\n      })\n    },\n  },\n}\n</script>\n\n<style>\ntextarea {\n  resize: vertical;\n}\ninput[type=\"number\"]::-webkit-inner-spin-button,\ninput[type=\"number\"]::-webkit-outer-spin-button {\n  -webkit-appearance: none;\n  margin: 0;\n}\n.hr_divider {\n  border-bottom: 2px solid #82868b;\n}\n</style>\n<style lang=\"scss\">\n@import \"@core/scss/vue/libs/vue-autosuggest.scss\";\n</style>\n"],"sourceRoot":"src/views/table/bs-table/onBoarding"}]}